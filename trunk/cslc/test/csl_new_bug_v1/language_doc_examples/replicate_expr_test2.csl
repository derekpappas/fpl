// replicate in connect
csl_unit u_1{
  csl_port p11(input,4);
  csl_port p12(output,32);
  u_1(){}
};

csl_unit u_2{
  csl_port p21(input,16);
  csl_port p22(output,64);
  u_1 u1_i;
  u_2(){
    p21.connect_by_name({4{u1_i.p11}});
    p22.connect_by_name({2{u1_i.p12}});
}
};
