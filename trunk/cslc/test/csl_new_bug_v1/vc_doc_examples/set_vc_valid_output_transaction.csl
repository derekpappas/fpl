//Monica

csl_unit dut{
  csl_port stim_in(input), stim_v(input), exp_d(output),      exp_v(output);
  csl_port clk(input);
  csl_signal clk1;
  dut(){
  clk.set_attr(clock);
  exp_d = exp_v;
  clk1 = clk;
  stim_in = stim_v;
 }
};
csl_signal r(wire);
csl_vector stim_vec{
  stim_vec(){
    set_unit_name(dut);
    set_direction(input);
    set_vc_valid_output_transaction(r);
  }
};
csl_vector exp_vec{
  exp_vec(){
   set_unit_name(dut);
   set_direction(output);
}
};
csl_testbench tb{
csl_signal clk(reg);
dut dut_1(.clk(clk));
tb(){
  clk.set_attr(clock);
  add_logic(clock,clk,100,ps);

}
};
