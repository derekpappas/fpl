//2nd param of set_dim_bitrange should be a simple bitrange

csl_multi_dim_bitrange mbr1(3);
mbr1.set_dim_width(0,3);
mbr1.set_dim_width(1,6);
mbr1.set_dim_width(2,10);

csl_multi_dim_bitrange mbr2(3);
