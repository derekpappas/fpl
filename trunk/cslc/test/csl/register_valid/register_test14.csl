//Petronela

csl_register r1{
  r1(){
    set_width(8);
    set_type(register);
  }
};

csl_unit u1{
  csl_port clk(input);
  csl_port p1(input);
  csl_port p2(input);
  csl_port p3(output,8);
  csl_port p6(input,8);
  r1 r1(.reset_(p1),.enable(p2),.clock(clk),.reg_out(p3),.reg_in(p6));
  u1(){
  }
};
