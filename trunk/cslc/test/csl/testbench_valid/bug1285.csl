csl_interface ifc_d {
  csl_port p_data_in(input,8);
  csl_port p_data_out(output,8);
  ifc_d(){
  }
};

csl_unit u_struc {
  ifc_d ifc_d0;
  csl_port clk(input);
  u_struc(){
      clk.set_attr(clock); 
 }
};

csl_vector v_stim{
    v_stim(){
    set_unit_name(u_struc);
    set_direction(input);
    }
  };

csl_vector v_exp{
  v_exp(){
  set_unit_name(u_struc);
  set_direction(output);
  }};

csl_unit a {
  csl_port p1(input,8);
  //csl_port p2(input,8);
  csl_port p3(output,8);
  //csl_port p4(output,8);
  csl_port p5(input);
  u_struc dut1(.ifc_d0.p_data_in(p1),.ifc_d0.p_data_out(p3),.clk(p5));
  a (){}
};

csl_testbench tb{
  csl_signal clk(reg);
  u_struc u_struc(.clk(clk));
  
  tb(){
    clk.set_attr(clock);
    add_logic(clock,clk,10,ns);
  }
};
