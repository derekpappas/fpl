//csl_isa_field_isa_field_oject(width[,enum_or_enum_item]).csl
//20 August 2007

csl_enum e_op{ 
  ALU,   
    MOV, 
    BR
};
csl_enum e_ALU{ 
  OR, 
    XOR, 
    AND, 
    ADD,        
    SUB
};
csl_isa_field f_op{
  f_op(){
    set_width(2);
    set_enum(e_op);
    set_type(opcode);
  }
}; 
csl_isa_field f_alu{
  f_alu(){
    set_width(3);
    set_enum(e_ALU);
    set_type(selector);
  }
};
csl_unit top {
};
