csl_interface ifc {
  csl_port x(input);
  csl_port y(output);
  ifc () {}
};

csl_unit a {
  ifc ifc0;
  ifc ifc1;
  a () {
    ifc1.reverse();
  }
};

csl_unit b {
  a a0;
  b () {}
};

csl_unit c {
  b b0;
  a a1;
  c () {}
};

csl_unit d {
  c c0;
  c c1;
  c c2;
  d () {
    c0.b0.a0.ifc0.x.connect_by_name(c1.a1.ifc1.x);
    c1.a1.ifc1.y.connect_by_name(c2.b0.a0.ifc0.y);
  }
};
