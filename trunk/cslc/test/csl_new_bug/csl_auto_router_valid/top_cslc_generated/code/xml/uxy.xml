<?xml version = "1.0" ?>
<?xml-stylesheet type="text/xsl" href="unit.xsl"?>
<design>
<unit name="uxy">
<interface name="irc" >
<port name="reset" offset="0" upper="0" lower="0" direction = "input" type = "wire" />
<port name="clock" offset="0" upper="0" lower="0" direction = "input" type = "wire" />
</interface>
<interface name="ifcxin" >
<port name="pinx" offset="0" upper="8-1" lower="0" direction = "input" type = "wire" />
<port name="piny" offset="0" upper="8-1" lower="0" direction = "input" type = "wire" />
</interface>
<interface name="iin1" >
<port name="pinx" offset="0" upper="8-1" lower="0" direction = "output" type = "wire" />
<port name="piny" offset="0" upper="8-1" lower="0" direction = "output" type = "wire" />
</interface>
<interface name="iin2" >
<port name="pinx" offset="0" upper="8-1" lower="0" direction = "output" type = "wire" />
<port name="piny" offset="0" upper="8-1" lower="0" direction = "output" type = "wire" />
</interface>
<port name="ar_tsel1_sel" offset="0" upper="2-1" lower="0" direction = "input" type = "wire" />
<port name="ar_tsel2_sel" offset="0" upper="2-1" lower="0" direction = "input" type = "wire" />
<instantiation type = "uu">
<instance name="ux">
<f2a formal = "iin.pinx" actual = "ifcxin.pinx" />
<f2a formal = "iin.piny" actual = "ifcxin.piny" />
<f2a formal = "iout.pinx" actual = "iin1.pinx" />
<f2a formal = "iout.piny" actual = "iin1.piny" />
<f2a formal = "irc" actual = "irc" />
<f2a formal = "sel" actual = "ar_tsel1_sel" />
</instance>
</instantiation>
<instantiation type = "uu">
<instance name="uy">
<f2a formal = "iin.pinx" actual = "ifcxin.pinx" />
<f2a formal = "iin.piny" actual = "ifcxin.piny" />
<f2a formal = "iout.pinx" actual = "iin2.pinx" />
<f2a formal = "iout.piny" actual = "iin2.piny" />
<f2a formal = "irc" actual = "irc" />
<f2a formal = "sel" actual = "ar_tsel2_sel" />
</instance>
</instantiation>
</unit>
</design>
