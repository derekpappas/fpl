//port to port connections

csl_unit test1{
  csl_port in1(input,2),in2(input,4);
  csl_port out1(output,2),out2(output,4);

  test1(){}
};
csl_unit top{
  csl_port p_in1(input,2),p_in2(input,4),p_out1(output,2),p_out2(output,4);
  test1 test1;
  top(){
    test1.in1.connect_by_name(p_in1);
    test1.in2.connect_by_name(p_in2);
    test1.out1.connect_by_name(p_out1);
    test1.out2.connect_by_name(p_out2);
  }};
