csl_unit u {
  u () {}
};

csl_memory_map_page mmp_page{
  mmp_page() {
    add_address_range(0,128);
    set_unit_name(u);
    set_endianess(big_endian);
    set_data_word_width(32);
  }
};

csl_memory_map mm{
  mm(){
    auto_gen_memory_map();
    set_type(hierarchical);
  }
};
