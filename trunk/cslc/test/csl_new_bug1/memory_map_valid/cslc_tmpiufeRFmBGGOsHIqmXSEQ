# "/opt/he_fpl_svn/fpl_repo/cslc/test/csl_new_bug1/memory_map_valid/./mpage_add.csl" 1



csl_register_file rf{
  rf(){
    set_width(8);
    set_depth(4);
  }
};

csl_fifo f{
  f(){
    set_width(4);
    set_depth(8);
    add_to_memory_map();
  }
};

csl_unit u{
  rf rf1;
  f f1;
  u(){}
};

csl_memory_map_page mpg{
  mpg(){
    set_data_word_width(32);
    add_address_range(10,120);
    set_unit_name(u);
    add(u.rf1,"reg_file",15);
    add(f1,"fifo",25);
  }
};

csl_memory_map mp{
  mp(){
  set_data_word_width(32);
  set_type(hierarchical);
  }
};
