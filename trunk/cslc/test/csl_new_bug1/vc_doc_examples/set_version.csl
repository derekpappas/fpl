//

csl_unit dut{
  csl_port stim_in(input), stim_v(input), exp_d(output),exp_v(output);
  csl_port clk(input);
  csl_signal s;
  dut(){
  clk.set_attr(clock);
  clk = s;
  stim_v = stim_in;
  exp_d = exp_v;
}
};
csl_vector stim_vec{
  stim_vec(){
    set_unit_name(dut);
    set_direction(input);
    set_version(2);
  }
};
csl_vector exp_vec{
  exp_vec(){
   set_unit_name(dut);
   set_direction(output);
}
};
csl_testbench tb{
csl_signal clk(reg);
dut dut_1(.clk(clk));
tb(){
  clk.set_attr(clock);
  add_logic(clock,clk,100,ps);

}
};
