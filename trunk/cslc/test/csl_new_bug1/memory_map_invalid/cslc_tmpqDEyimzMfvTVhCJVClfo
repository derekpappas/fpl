# "/opt/he_fpl_svn/fpl_repo/cslc/test/csl_new_bug1/memory_map_invalid/./unit_for_2mpags.csl" 1




csl_register reg{
  reg(){
    set_width(32);
    set_type(counter);
    add_logic(count_direction,up);
  }
};

csl_unit u{
  reg reg;
  u(){}
};

csl_memory_map_page mpag1{
  mpag1(){
    add_address_range(0,156);
    set_unit_name(u);
    add(reg,"counter",6);
  }
};

csl_memory_map_page mpag2{
  mpag2(){
    add_address_range(0,128);
    set_unit_name(u);
  }
};

csl_memory_map mmap{
  mmap(){
    set_type(hierarchical);
  }
};
