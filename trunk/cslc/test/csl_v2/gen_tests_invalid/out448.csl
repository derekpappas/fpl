csl_fifo fv{
   fv( ){
     add_logic( parallel_output, all);
     add_logic( parallel_input, all);
     add_logic( stall);
     add_logic( almost_empty, 5);
  }
}
;
csl_memory_map_page ea{
    ea( ){
     set_next_address( 3);
     add_reserved_address_range( 8, 3);
     add( fv, "ju", 9);
     get_lower_bound( );
     get_data_word_wodth( );
     set_endianess( big_endian);
     get_endianess( );
     get_symbol_lenght( );
  }
}
;
csl_memory_map_page ge{
    ge( ){
     get_next_address( );
     set_access_rights( access_read, access_read_write);
     add_reserved_address_range( 9, 4);
     get_upper_bound( );
     get_aligment( );
     set_symbol_max_lenght( );
  }
}
;
csl_memory_map_page sw{
    sw( ){
     add_address_range( 9, 2);
     set_address_increment( 3);
     get_address_increment( );
     add_reserved_address_range( 9, 8);
     get_lower_bound( );
     set_data_word_width( 0);
     get_aligment( );
  }
}
;
