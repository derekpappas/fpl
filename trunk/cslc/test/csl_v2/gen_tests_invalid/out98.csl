csl_memory_map_page pa{
    pa( ){
     set_address_increment( 9);
     get_address_increment( );
     set_next_address( 8);
     get_upper_bound( );
     set_data_word_width( 0);
     set_aligment( 4);
     set_endianess( little_endian);
     get_endianess( );
  }
}
;
csl_memory_map_page qd{
    qd( ){
     add_address_range( 4, 7);
     get_address_increment( );
     get_next_address( );
     set_access_rights( none, access_read);
     add( yq, "yw");
     add( pa);
     get_upper_bound( );
     set_data_word_width( 3);
     get_aligment( );
     get_endianess( );
  }
}
;
