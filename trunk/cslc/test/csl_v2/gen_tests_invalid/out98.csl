csl_register rc{
    rc( ){
     set_range( [1:9]);
     set_width( 6);
     set_value( 1);
     reset_value( 6);
     clear_value( 0);
     set_lock_enable_bit( 9);
     get_lock_enable_bit( );
     add_logic( cnt_dir_signal);
     add_logic( inc_signal);
     add_logic( dec_signal);
  }
}
