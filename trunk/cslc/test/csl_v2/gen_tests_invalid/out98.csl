csl_memory_map mi{
  pa fp;
  qd no;
  pa jr;
    mi( ){
     auto_gen_memory_map( );
     set_top_unit( elxqgc);
.add_to_memory_map( access_read_write); 
     set_type( hierach);
     set_data_word_width( 4);
     get_data_word_width( );
     set_prefix( qi);
     get_prefix( );
     set_suffix( db);
  }
}
