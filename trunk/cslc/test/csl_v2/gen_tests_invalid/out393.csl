csl_memory_map_page cd{
    cd( ){
     set_address_increment( 4);
     get_next_address( );
     set_access_rights( access_write, access_read);
     get_lower_bound( );
     set_data_word_width( 2);
     get_endianess( );
  }
}
;
csl_memory_map_page at{
    at( ){
     set_address_increment( 0);
     get_address_increment( );
     get_next_address( );
     set_access_rights( access_read_write, access_read);
     add( cd);
     get_data_word_wodth( );
     set_aligment( 9);
     get_aligment( );
     set_endianess( little_endian);
  }
}
;
csl_memory_map_page ea{
  cd lu;
