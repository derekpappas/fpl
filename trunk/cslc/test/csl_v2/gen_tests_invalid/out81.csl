csl_memory_map_page bh{
    bh( ){
     add_address_range( 6, 6);
     set_access_rights( access_read, none);
     add_reserved_address_range( 6, 2);
     set_data_word_width( 3);
     set_aligment( 2);
     get_endianess( );
  }
}
;
csl_memory_map_page fo{
  bh uc;
    fo( ){
     get_address_increment( );
     set_next_address( 7);
     add_reserved_address_range( 7, 0);
     add( bh);
     get_lower_bound( );
     set_aligment( 0);
     get_aligment( );
     set_symbol_max_lenght( );
     get_symbol_lenght( );
  }
}
;
csl_memory_map_page gr{
  bh aj;
  bh in;
    gr( ){
     set_address_increment( 3);
     get_next_address( );
     set_access_rights( none, access_read_write);
     add( rv, "gf", 4);
     add( bhfo);
     get_lower_bound( );
     get_upper_bound( );
     get_data_word_wodth( );
     get_endianess( );
     get_symbol_lenght( );
  }
}
;
csl_memory_map rq{
    rq( ){
     auto_gen_memory_map( );
     set_top_unit( sgfm);
bhfogr.add_to_memory_map( access_write); 
     set_type( hierach);
bhfogr.set_access_rights_enum( bhfogr); 
     set_data_word_width( 6);
     set_prefix( lo);
     get_prefix( );
  }
}
;
csl_register mw{
    mw( ){
     index_data_pair( rq, 6);
     add_logic( neg_output);
     add_logic( serial_input);
     add_logic( rd_en);
     add_logic( gray_output);
     set_count_amount( 9);
     set_count_direction( 3);
  }
}
;
csl_register sr{
    sr( ){
