csl_unit u9_t3 {

  csl_signal sgn9_1_t3;
  csl_signal sgn9_2_t3;
  csl_signal sgn9_3_t3;
  csl_signal sgn9_4_t3;
  csl_signal sgn9_5_t3;

  u9_t3() {
    sgn9_1_t3.set_range(4,0);
    sgn9_2_t3.set_range(1,1);
    sgn9_3_t3.set_range(32,0);
    sgn9_4_t3.set_range(2,0);
  }
};

csl_unit u10_t3 {

  csl_signal sgn10_1_t3(4);
  csl_signal sgn10_2_t3(6,2),sgn10_3_t3(3,1);

  u10_t3() {
    sgn10_1_t3.set_range(7,1);
    sgn10_2_t3.set_range(4,1);
    sgn10_3_t3.set_range(sgn10_1_t3.get_upper(), sgn10_2_t3.get_lower());
    sgn10_3_t3.set_range(sgn10_1_t3.get_lower(), sgn10_2_t3.get_upper());
  }
};

csl_unit u11_t3 {

  csl_signal sgn11_1_t3,sgn11_2_t3, sgn11_4_t3;
  csl_signal sgn11_3_t3(2,5),sgn11_5_t3,sgn11_6_t3(2,6);
  csl_signal sgn11_7_t3,sgn11_8_t3,sgn11_9_t3(4,3),sgn11_10_t3,sgn11_11_t3;

  u11_t3() {
    sgn11_1_t3.set_range(u10_t3.sgn10_1_t3.get_width(),4);
    sgn11_4_t3.set_range(sgn11_2_t3.get_width(), 6);
    sgn11_4_t3.set_range(sgn11_2_t3.get_width(), 6);
    sgn11_5_t3.set_range(sgn11_3_t3.get_width(), 6);
    sgn11_5_t3.set_range(1,3);
    sgn11_7_t3.set_range(sgn11_6_t3.get_width(), 10);
    sgn11_7_t3.set_range(sgn11_6_t3.get_width(), 10);
    sgn11_8_t3.set_range(sgn11_6_t3.get_width(), 10);
    sgn11_8_t3.set_range(sgn11_6_t3.get_width(), 10);
    sgn11_10_t3.set_range(sgn11_9_t3.get_width(), 8);
    sgn11_10_t3.set_range(sgn11_9_t3.get_width(), 8);
    sgn11_11_t3.set_range(sgn11_9_t3.get_width(), 8);
    sgn11_11_t3.set_range(sgn11_9_t3.get_width(), 8);
  }
};

csl_unit u12_t3 {

  csl_signal sgn12_1_t3, sgn12_3_t3(4,2), sgn12_4_t3;
  csl_signal sgn12_2_t3,sgn12_5_t3;

  u12_t3() {
    sgn12_1_t3.set_range(u11_t3.sgn11_3_t3.get_width(), 3);
    sgn12_1_t3.set_range(u11_t3.sgn11_3_t3.get_width(), 3);
    sgn12_2_t3.set_offset(2);
    sgn12_3_t3.set_offset(2);
    sgn12_4_t3.set_range(7, 8);
    sgn12_4_t3.set_range(0, 2);
    sgn12_4_t3.set_offset(1);
    sgn12_5_t3.set_range(sgn12_2_t3.get_offset(), 8);
    sgn12_5_t3.set_range(sgn12_4_t3.get_offset(), 8);
  }
};

csl_unit u13_t3 {

  csl_signal sgn13_1_t3, sgn13_2_t3, sgn13_4_t3;
  csl_signal sgn13_5_t3, sgn13_6_t3, sgn13_7_t3;

  u13_t3() {
     sgn13_1_t3.set_range(u12_t3.sgn12_4_t3.get_offset(), 1);
     sgn13_1_t3.set_range(u12_t3.sgn12_2_t3.get_offset(), 1);
    sgn13_4_t3.set_offset(4);
     sgn13_5_t3.set_width(sgn13_4_t3.get_offset());
    sgn13_6_t3.set_range(u12_t3.sgn12_2_t3.get_offset(),sgn13_4_t3.get_offset());
    sgn13_7_t3.set_range(sgn13_1_t3.get_width(),sgn13_1_t3.get_lower());
    sgn13_2_t3.set_range(sgn13_1_t3.get_width(),sgn13_1_t3.get_upper());
  }
};
