//         - param 1 : dimension index(dimension indexes start from 0 and go up to num_of_dims-1)
//         - returns : numeric expresion that represents the lower index of the specified dimension  

csl_multi_dim_bitrange obj_name(10);

obj_name.set_dim_range(1, 2, 20);
obj_name.set_dim_range(2, 3, 30);
obj_name.set_dim_range(3, 4, 40);
obj_name.set_dim_range(4, 5, 50);
obj_name.set_dim_range(5, 6, 60);
obj_name.set_dim_range(6, 7, 70);
obj_name.set_dim_range(7, 8, 80);
obj_name.set_dim_range(8, 9, 90);
obj_name.set_dim_range(9, 10, 100);

csl_multi_dim_bitrange obj_name1(6);
obj_name1.set_dim_width(0, obj_name.get_dim_lower(0));
obj_name1.set_dim_width(1, obj_name.get_dim_lower());
obj_name1.set_dim_width(2, obj_name.get_dim_lower);
obj_name1.set_dim_width(3, obj_name.get_dim_lower(3, 3));
obj_name1.set_dim_width(4, obj_name.get_dim_lower(bitrange));
