## damned ugly hack ..
#ifeq (gcj,javac)
#	antlr_java_FILES = $(antlr_java1_FILES)
#else
	antlr_java_FILES = $(antlr_java1_FILES) $(antlr_swing_FILES)
#endif

## rule how to compile java files 
/opt/tools/antlr-2.7.7/antlr/antlr.jar :: $(antlr_java_FILES)
	@ -/bin/rm -r -f $@
	@ /bin/sh /opt/tools/antlr-2.7.7/scripts/javac.sh $?
	@ /bin/sh /opt/tools/antlr-2.7.7/scripts/jar.sh $@ antlr
	@ test -f $@ || { exit 1; }

## how ANTLR grammar files ..
$(antlr_java_action_FILES) : /opt/tools/antlr-2.7.7/antlr/../antlr/actions/java/action.g
	@cd actions/java && /bin/sh /opt/tools/antlr-2.7.7/scripts/antlr.sh $<

$(antlr_cpp_action_FILES) : /opt/tools/antlr-2.7.7/antlr/../antlr/actions/cpp/action.g
	@cd actions/cpp && /bin/sh /opt/tools/antlr-2.7.7/scripts/antlr.sh $<

$(antlr_csharp_action_FILES) : /opt/tools/antlr-2.7.7/antlr/../antlr/actions/csharp/action.g
	@cd actions/csharp && /bin/sh /opt/tools/antlr-2.7.7/scripts/antlr.sh $<

$(antlr_python_action_FILES) : /opt/tools/antlr-2.7.7/antlr/../antlr/actions/python/action.g
	@cd actions/python && /bin/sh /opt/tools/antlr-2.7.7/scripts/antlr.sh $<

$(antlr_python_code_FILES) : /opt/tools/antlr-2.7.7/antlr/../antlr/actions/python/code.g
	@cd actions/python && /bin/sh /opt/tools/antlr-2.7.7/scripts/antlr.sh $<

$(antlr_antlr_FILES) : /opt/tools/antlr-2.7.7/antlr/../antlr/antlr.g
	/bin/sh /opt/tools/antlr-2.7.7/scripts/antlr.sh $<

$(antlr_tokdef_FILES) : /opt/tools/antlr-2.7.7/antlr/../antlr/tokdef.g
	/bin/sh /opt/tools/antlr-2.7.7/scripts/antlr.sh $<

clean ::
	/bin/rm -r -f *.class antlr /opt/tools/antlr-2.7.7/antlr/antlr.jar

distclean :: clean
	/bin/rm -r -f $(antlr_gen_FILES)

## install antlr.jar in libdirectory if exists
install ::	/opt/tools/antlr-2.7.7/antlr/antlr.jar 
	-$(MKDIR) -p "$(libdir)"
	-$(MKDIR) -p "$(datadir)/$(versioneddir)"
	-test -f /opt/tools/antlr-2.7.7/antlr/antlr.jar && {\
	 $(INSTALL) -m 444 /opt/tools/antlr-2.7.7/antlr/antlr.jar "$(datadir)/$(versioneddir)" ; \
	 $(INSTALL) -m 444 /opt/tools/antlr-2.7.7/antlr/antlr.jar "$(libdir)" ; \
	}

distclean :: clean
	/bin/rm -r -f Makefile Version.java

## get configured dependencies

## --*- Makefile -*--
## Make listed targets even in case a file with same name exists.
.PHONY: \
 this \
 all clean install test distclean \
 this-all this-clean this-install this-test this-distclean \
 all-this clean-this install-this test-this distclean-this \
 $(eof)

## delete suffix rules - shortens output when using -d and is not
## used anyway (and should not be used).
.SUFFIXES:

Makefile :: /opt/tools/antlr-2.7.7/scripts/../$(subdir)/Makefile.in  \
            /opt/tools/antlr-2.7.7/scripts/config.deps \
					  /opt/tools/antlr-2.7.7/scripts/config.make \
					  /opt/tools/antlr-2.7.7/scripts/config.vars \
            /opt/tools/antlr-2.7.7/config.status
	@echo "*** update $(subdir)/Makefile"
	@cd /opt/tools/antlr-2.7.7 && CONFIG_FILES="$(subdir)/Makefile" /bin/sh ./config.status -q

/opt/tools/antlr-2.7.7/scripts/config.deps : \
	/opt/tools/antlr-2.7.7/scripts/../scripts/config.deps.in
	@echo "*** update $@"
	@cd /opt/tools/antlr-2.7.7 && CONFIG_FILES=scripts/config.deps /bin/sh ./config.status -q

/opt/tools/antlr-2.7.7/scripts/config.make : \
	/opt/tools/antlr-2.7.7/scripts/../scripts/config.make.in
	@echo "*** update $@"
	@cd /opt/tools/antlr-2.7.7 && CONFIG_FILES=scripts/config.make /bin/sh ./config.status -q

/opt/tools/antlr-2.7.7/scripts/config.vars : \
	/opt/tools/antlr-2.7.7/scripts/../scripts/config.vars.in
	@echo "*** update $@"
	@cd /opt/tools/antlr-2.7.7 && CONFIG_FILES=scripts/config.vars /bin/sh ./config.status -q

/opt/tools/antlr-2.7.7/config.status : \
	/opt/tools/antlr-2.7.7/scripts/../configure
	@echo "*** reconfigure $@ - stay tuned .."
	@cd /opt/tools/antlr-2.7.7 && /bin/sh ./config.status -q --recheck
	@echo "*** update all configured files .."
	@cd /opt/tools/antlr-2.7.7 && /bin/sh ./config.status -q

### In a pure devel mode there should be also a dependency listed on how
### to make configure out of  configure.in.  This  requires that usr has 
### m4  and  autoconf (proper version) installed. Appropriate checks are 
### not done in configure. If so, then uncomment next lines:
###
###   @rule_configure_configure_in@
###

