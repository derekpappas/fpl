//the memory map page has no address range set

csl_fifo f1{
  f1 () {
    set_width(32);
    set_depth(8);
  }
};

csl_unit u{
f1 f1;
u(){}
};

csl_memory_map_page mpg{
  mpg(){
    // the add_address_range(..) command should be here
    set_unit_name(u);
    set_data_word_width(32);
  }
};

csl_memory_map mmap{
  mmap(){
    set_type(hierarchical);
  }
};
