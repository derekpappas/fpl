csl_interface ifc {
  csl_port x(input),y(output);
  ifc () {}
};

csl_unit u1 {
  csl_port x1(input);
  ifc ifc0;
  u1 () {}
};

csl_unit u2 {
  csl_port y2(output);
  u2 () {}
};

csl_unit u3 {
  u1 u1;
  u2 u2;
  csl_port x3(input);
  u3 () {}
};

csl_unit u4 {
  u3 u3;
  csl_port a(input);
  u4 () {}
};

csl_unit u {
  u4 u4;
  csl_signal s;
  u () {
    u4.u3.u1.ifc0.x.connect_by_name(u4.u3.u2.y2);
    u4.u3.u2.y2.connect_by_name(u4.u3.u1.x1);
    u4.u3.x3.connect_by_name(u4.u3.u1.ifc0.x);
    u4.a.connect_by_name(u4.u3.x3);
    s.connect_by_name(u4.a);
   }
};
