csl_enum op_enum{
	CALL = 1,
	SETHI_Branches = 0,
	Arithmetic = 2,
	Logical = 3
};

csl_enum op2_enum{
	ILLTRAP = 0,
	BPcc = 1,
	Bicc = 2,
	BPr = 3,
	SETHINOP = 4,
	FBPfcc = 5,
	FBfcc = 6 
};
csl_isa_field op(2,op_enum);
op.set_type(subopcode);
csl_isa_field op2(3, op2_enum);
op2.set_type(subopcode);

csl_isa_field fmt2Base{
	op op_inst;
	op2 op2_inst;
	fmt2Base(){
		set_width(32);
		set_position(op_inst, 30);
		set_position(op2_inst, 22);
                set_type(opcode);
	}
};


csl_unit top {
};
