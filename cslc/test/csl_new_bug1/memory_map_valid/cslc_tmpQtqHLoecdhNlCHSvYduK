csl_register_file rf{
	rf(){
		set_width(32);
		set_depth(128);
	}
};

csl_unit u1{
  rf rf;
  u1(){

  }
};

csl_memory_map_page mpage{
  mpage(){
    add_address_range(0,200);
    set_unit_name(u1);
    add(u1.rf,"rf",30);
  }
};

csl_memory_map mmap{
	mmap(){
		set_data_word_width(32);
		set_type(hierarchical);
	}
};
