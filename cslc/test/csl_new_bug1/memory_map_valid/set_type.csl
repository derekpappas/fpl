csl_fifo f{
  f(){
    set_width(4);
    set_depth(8);
  }
};

csl_unit u{
  f f1;
  u(){}
};

csl_memory_map_page mpag{
  mpag(){
    add_address_range(0,63);
    set_unit_name(u);
    set_address_increment(2);
    set_data_word_width(32);
    }
};

csl_memory_map mmap{
   mmap(){
      set_data_word_width(32);
      set_type(hierarchical);
}
};
