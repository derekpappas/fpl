csl_isa_instruction_format ll{
    ll( ){
     set_width( 1);
     generate_decoder( wbrxkb);
  }
}
;
csl_isa_instruction do : ll{
    do( ){
    set_asm_mnemonic( csl_list "dg", "oc", "fh", "ge", "tw", "av", "uv");
  }
}
;
csl_isa_instruction eg : ll{
    eg( ){
    set_asm_mnemonic( csl_list "lr", "au", "xt", "ra", "rm", "vx", "gg", "du", "po");
  }
}
