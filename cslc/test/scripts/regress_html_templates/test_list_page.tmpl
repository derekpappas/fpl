      <td class="<TMPL_VAR NAME="compilerPhasePassedPercentageCSSClass">" name="overall">    <TMPL_VAR NAME="compilerPhasePassedPercentage"             > </td>
      <td class="<TMPL_VAR NAME="cslcCompileGenVerilogStatusCSSClass">"> <a href="<TMPL_VAR NAME="cslcCompileVerilogLog">" ><TMPL_VAR NAME="cslcCompileVerilogCode"></a></td> 
      <td class="<TMPL_VAR 
NAME="gccCompileGenVHDLStatusCSSClass">" > <a href="<TMPL_VAR NAME="gccCompileVHDLLog" >" ><TMPL_VAR 
NAME="gccCompileVHDLCode" ></a></td>          
