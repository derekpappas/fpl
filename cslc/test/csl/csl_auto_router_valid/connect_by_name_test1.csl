//Monica

csl_unit u1{
  csl_port p1(input,8);
  csl_port p2(output,8);
  csl_signal s1(8);
  u1(){
    s1=p1;
    p2=s1;
  }};
csl_unit u2{
  csl_port p3(input,8);
  csl_port p4(output,8);
  u1 u1i;
  u2(){
    p3.connect_by_name(u1i.p1);    //by name
    u1i.p2.connect_by_name(p4);   //by name
  }
};
