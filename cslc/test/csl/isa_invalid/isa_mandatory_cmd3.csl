//set_type -mandatory command for isa_element
//danielt

csl_isa_field  fld_op{
  fld_op(){
    set_width(2);
    set_type(opcode);
  }
};
csl_isa_element fmtbase {
  fmtbase () {
    set_type(root_format);
    set_width(32);
    
  }
};
csl_isa_element fmtsubbase : fmtbase {
  fld_op fld_op;
  fmtsubbase () {
    set_position(fld_op, 16);
  }
};
