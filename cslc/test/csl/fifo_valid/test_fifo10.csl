csl_fifo f1{
  f1(){
    set_width(4);
    set_depth(4);
    set_physical_implementation(ffa);
    add_logic(priority_bypass);
    add_logic(sync_fifo);
    add_logic(depth_extend,8);
    add_logic(width_extend,6);
    add_logic(wr_hold,9);
    //    set_prefix("abc");
  }
};
csl_unit u1{
  csl_port p1(input);
  csl_port p2(input);
  csl_port p3(output);
  csl_port p4(output);
  csl_port p5(output,4);
  csl_port p6(input,4);
  csl_port p7(input);
  csl_port p8(input);
  csl_port p9(output);
  f1 ff1(.push(p1),.pop(p2),.full(p3),.empty(p4),.rd_data(p5),.wr_data(p6),.reset_(p7),.clock(p8),.valid(p9));
  u1(){
  }
};
