//register does not have a type specified

csl_register reg{
  reg(){
    set_width(16);
    //type of register should be here
  }
};

csl_unit u{
  reg reg1;
  u(){}
};

csl_memory_map_page mpg{
  mpg(){
    add_address_range(0,512);
    add(u.reg1,"reg",100);
  }
};

csl_memory_map mmap{
  mpg mpg1;
  mmap(){
    set_type(hierarchical);
  }
};
