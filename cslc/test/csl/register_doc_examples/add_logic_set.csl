csl_register reg1{
  reg1(){
    set_type(register);
    set_width(8);
    add_logic(set,3);
  }
};

csl_unit u{
  csl_port clk(input);
  csl_port p1(input);
  csl_port p2(input);
  csl_port p3(output,8);
  csl_port p4(input,8);
  csl_port p5(input);
  reg1 reg1(.reset_(p1),.enable(p2),.clock(clk),.reg_out(p3),.reg_in(p4),.set(p5));
  u(){
    clk.set_attr(clock);
  }
};
