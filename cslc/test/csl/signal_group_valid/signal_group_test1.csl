//Petronela
csl_signal_group sg1{
  csl_signal s1;
  csl_signal s2;
  sg1(){
    s1 =s2;
  }
};
csl_signal_group sg2{
  sg1 sg1inst;
  csl_signal s3;
  csl_signal s4;
  sg2(){
    s3 =s4;
  }
};

csl_unit a{
  csl_port p1(input);
  csl_port p2(output);
  a(){}
};
csl_unit u{
  sg2 sg2inst;
  a a0(.p1(sg2inst.s3),.p2(sg2inst.s4));
  a a1(.p1(sg2inst.sg1inst.s1),.p2(sg2inst.sg1inst.s2));
  u(){
    
  }
};
