//oanab
//27 oct 2007
//fifo is added outside the specified address range

csl_fifo ff{
  ff(){
    set_depth(16);
    set_width(4);
  }
};

csl_unit u{
  ff ff;
  u(){}
};

csl_memory_map_page mpg{
  mpg(){
    add_address_range(0,128);
    add(u.ff,"ff1",130); // the base address should be between 0 and 112
  }
};

csl_memory_map mmp{
  mmp(){
    set_type(hierarchical);
  }
};
