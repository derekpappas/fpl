// Creating a hid item using signal groups in signal groups

csl_signal_group signalGroupA {
};

csl_signal_group signalGroupB {
};


csl_signal_group signalGroupAB {
  signalGroupA signalGroupA0;
  signalGroupA signalGroupB0;
};


csl_signal_group signalGroupC {
  signalGroupAB signalgroupAB0;
};


csl_unit unitTop {

  signalGroupC signalGroupC0; 

  unitTop() {
    signalGroupC0.signalGroupAB0.signalGroupA0.generate_individual_rtl_signals(on);
    signalGroupC0.signalGroupAB0.signalGroupB0.generate_individual_rtl_signals(on);                              
  }
};
