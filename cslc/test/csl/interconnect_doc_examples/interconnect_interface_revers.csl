csl_interface p{
  csl_port x(output,1),y(output,1),z(output,1),a(input,1), b(input,1), c(input,1);
  p(){}
};

csl_unit P{
  p p0;
  P(){
  }
};

csl_unit Q{
  csl_port p1(input),p2(input),p3(input),p4(output),p5(output),p6(output);
  P P1(.p0.x(p1),.p0.y(p2),.p0.z(p3),.p0.a(p4),.p0.b(p5),.p0.c(p6));
  //p q0;
  Q(){
    P1.p0.reverse();
 }
};	
