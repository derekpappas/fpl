csl_unit a {
  csl_port p1(input,2);
  csl_port p2(input);
  a () {}
};

csl_unit b {
  a a0;
  b () {}
};

csl_unit c {
  b b0;
  csl_signal s(6);        
  c () {
    s.connect_by_name({2{a0.p1,a0.p2}});
  }
};


