//------------------------------------------------------------------//
// ROOT FORMAT                                                      //
//------------------------------------------------------------------//

csl_isa_element isa_root{
  isa_root(){
    set_type(root_format);
    set_width(32);
  }
};
csl_isa_element form : isa_root{
  op op;
  form(){
    set_type(instr_format);
    set_position(op,28);
  }
};

//------------------------------------------------------------------//
// FORMAT 1 (op=1): CALL                                            //
//------------------------------------------------------------------//

csl_isa_element format1:form{
  disp30 disp30;
  format1(){
    set_type(instr_format);
    set_next(op,disp30);
  }
};

//------------------------------------------------------------------//
// FORMAT 2 (op=0):SETHI & Branches(Bicc, BPcc, BPr, FBfcc, FBPfcc) //
//------------------------------------------------------------------//
csl_isa_element fmt2Base:form{	
  op2 op2;
  fmt2Base(){
    set_type(instr_format);
    set_position(op2,20);
   
	}
};

csl_isa_element fmt21Base:fmt2Base{
  a a_inst_fmt21Base;
  cond cond;
  fmt21Base(){
    set_type(instr_format);
    set_next(op,               a_inst_fmt21Base);
    set_next(a_inst_fmt21Base,      cond);
  }
};

csl_isa_element fmt22:fmt21Base{
  cc1 cc1_inst_fmt22;
  cc0 cc0_inst_fmt22;
  p p_inst_fmt22;
  disp19 disp19_inst_fmt22;
  fmt22(){
    set_type(instr_format);
    set_next(op2,                   cc1_inst_fmt22);
    set_next(cc1_inst_fmt22,        cc0_inst_fmt22);
    set_next(cc0_inst_fmt22,        p_inst_fmt22);
    set_next(p_inst_fmt22,          disp19_inst_fmt22);
  }
};

csl_isa_element fmt22FBPfcc:fmt22{
  fmt22FBPfcc(){
    set_type(instr_format);
  }
};

csl_isa_element fmt23:fmt2Base{
  a a_inst_fmt23;
  _0 _0_inst_fmt23;
  rcond rcond;
  d16hi d16hi_inst_fmt23; 
  p p_inst_fmt23;
  rs1 rs1_inst_fmt23;
  d16lo d16lo_inst_fmt23;
  fmt23(){
    set_type(instr_format);
    set_next(op,               a_inst_fmt23);
    set_next(a_inst_fmt23,          _0_inst_fmt23);
    set_next(_0_inst_fmt23,         rcond);
    set_next(op2,                   d16hi_inst_fmt23);
    set_next(d16hi_inst_fmt23,      p_inst_fmt23);
    set_next(p_inst_fmt23,          rs1_inst_fmt23);
    set_next(rs1_inst_fmt23,        d16lo_inst_fmt23);
	}
};


