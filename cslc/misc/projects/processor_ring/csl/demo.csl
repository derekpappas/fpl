csl_interface m2s_ifc { 
csl_port  clk(output);             // mitch to stella ex bus clock
csl_port  cmd(output);             // mitch to stella ex bus command
csl_port  ready(input);           // mitch to stella ex bus ready
csl_port  valid(output);           // mitch to stella ex bus valid
csl_port  d(output,16);               // mitch to stella ex bus data
csl_port  rsrvd(input,3);           // mitch to stella ex bus reserved

m2s_ifc () { }
};

// for IO pads
csl_interface m2s_out_ifc {
m2s_ifc m2s_ifc;
m2s_out_ifc() {
m2s_ifc.reverse();
}
};

csl_interface s2m_ifc { 
csl_port  clk(input);             // stella to mitch ex bus clock
csl_port  cmd(input);             // stella to mitch ex bus command
csl_port  ready(output);           // stella to mitch ex bus ready
csl_port  valid(input);           // stella to mitch ex bus valid
csl_port  d(input, 16);               // stella to mitch ex bus data
csl_port  rsrvd(input,3);           // stella to mitch ex bus reserved
s2m_ifc () { }
};

// for IO pads
csl_interface s2m_out_ifc {
s2m_ifc s2m_ifc;
s2m_out_ifc() {
s2m_ifc.reverse();
}
};

csl_interface stella_ifc { 
csl_port   ce_n(output);            // stella fpga programming
csl_port   confdone(input);        // stella fpga programming
csl_port   config_n(output);        // stella fpga programming
csl_port   dclk(output);            // stella fpga programming
csl_port   devclr_n(output);        // stella fpga programming
csl_port   happy(input);           // stella fpga programming
csl_port   initdone(input);        // stella fpga programming
csl_port   msel(output,4);            // stella fpga programming
csl_port   status_n(input);        // stella fpga programming
stella_ifc () { }
};

// for IO pads
csl_interface stella_out_ifc {
stella_ifc stella_ifc;
stella_out_ifc() {
stella_ifc.reverse();
}
};

csl_interface dsp_tx_ifc { 
csl_port   clkrs0(output);             // blackfin rx sport
csl_port   dpr0(output);               // blackfin rx sport
csl_port   dsr0(output);               // blackfin rx sport
csl_port   fsr0(output);               // blackfin rx sport
dsp_tx_ifc () { }
};

// for IO pads
csl_interface dsp_tx_out_ifc { 
dsp_tx_ifc dsp_tx_ifc;
dsp_tx_out_ifc () {
  dsp_tx_ifc.reverse();
}
};

csl_interface dsp_rx_ifc { 
csl_port   clkts0(input);             // blackfin tx sport
csl_port   dpt0(input);               // blackfin tx sport
csl_port   dst0(input);               // blackfin tx sport
csl_port   fst0(input);               // blackfin tx sport
dsp_rx_ifc () { }
};


// for IO pads
csl_interface dsp_rx_out_ifc { 
dsp_rx_ifc dsp_rx_ifc;
dsp_rx_out_ifc () {
dsp_rx_ifc.reverse();
}
};

csl_interface dsp_spi_ifc { 
csl_port   bootcs(input);          // blackfin boot spi cs
csl_port   sck(input);             // blackfin boot spi clk
csl_port   miso(output);           // blackfin boot spi data
csl_port   mosi(input);            // blackfin boot spi data
dsp_spi_ifc () { }
};

// for IO pads
csl_interface dsp_spi_out_ifc { 
dsp_spi_ifc dsp_spi_ifc;
dsp_spi_out_ifc() {
dsp_spi_ifc.reverse();
}
};

csl_interface enet_lcl_ifc { 
csl_port clk25e6ltx(input);        // local enet mac tx clock
csl_port txd(output,4);              // local enet mac tx data
csl_port txen(output);             // local enet mac tx enable
csl_port txer(output);             // local enet mac tx error
csl_port clk25e6lrx(input);        // local enet mac rx clock
csl_port rxd(input, 4);              // local enet mac rx data
csl_port rxdv(input);             // local enet mac rx data valid
csl_port rxer(input);             // local enet mac rx error
csl_port crs(input);              // local enet mac carrier sense
csl_port col(input);              // local enet mac 
enet_lcl_ifc () { }
};

csl_interface enet_ulink_ifc { 
csl_port          clk25e6utx(input    );  // uplink enet mac tx clock
csl_port          txd      (output, 4);              // uplink enet mac tx data
csl_port          txen(output);             // uplink enet mac tx enable
csl_port          txer(output);             // uplink enet mac tx error
csl_port          clk25e6urx(input);        // uplink enet mac rx clock
csl_port          rxd(input, 4);              // uplink enet mac rx data
csl_port          rxdv(input);             // uplink enet mac rx data valid
csl_port          rxer(input);             // uplink enet mac rx error
csl_port          crs(input);              // uplink enet mac carrier sense
csl_port          col(input);              // uplink enet mac 
enet_ulink_ifc () { }
};

csl_interface tflsh_ifc { 
csl_port         clk25e6(output);          // Transflash Clock
csl_port         cmd(inout);              // transflash command
csl_port         d(inout);                // transflash data
tflsh_ifc () { }
};

csl_interface sram_ifc { 
csl_port         clk133e6(inout);          // sram cLock
csl_port         ce_n(output);              // sram chip sel
csl_port         oe_n(output);              // sram csl_output en
csl_port         we_n(output);              // sram write enable
csl_port         bwa_n(output);             // sram byte write enable
csl_port         bwb_n(output);             // sram byte write enable
csl_port         bwc_n(output);             // sram byte write enable
csl_port         bwd_n(output);             // sram byte write enable
csl_port         a(output, 18);                 // sram address
csl_port         dqa(inout, 9);               // sram data
csl_port         dqb(inout, 9);               // sram data
csl_port         dqc(inout, 9);               // sram data
csl_port         dqd(inout, 9);               // sram data
sram_ifc () { }
};

csl_interface sram_out_ifc { 
sram_ifc sram;
sram_out_ifc () { }
};

csl_interface cpu_ifc { 
csl_port      poreset_n(output);          // cpu power on reset
csl_port      hreset_n(input);           // cpu hard reset
csl_port      sreset_n(input);           // cpu soft reset
csl_port      csboot_n(input);           // cpu boot chip sel 
csl_port      cs_n(input);               // cpu fpga chip sel 
csl_port      clk66e6(input);            // cpu bus clock
csl_port      a(input, 31,11);                  // cpu address
csl_port      d(inout, 32);                  // cpu data
csl_port      ts_n(input);               // cpu transfer start
csl_port      tsiz(input,3);               // cpu transfer size
csl_port      rd_wr_n(input);            // cpu read/write
csl_port      burst_n(input);            // cpu burst select
csl_port      bdip_n(input);             // cpu burst data in progress
csl_port      bi_n(output);               // cpu burst inhibit
csl_port      ta_n(output);               // cpu transfer ack
csl_port      tea_n(input);              // cpu transfer error ack
csl_port      irq_n(output, 8);              // cpu interrupts
cpu_ifc () { }
};

csl_interface i2c_ifc { 
csl_port         dat_400(inout);            // i2c 400khz data
csl_port         clk_400(output);            // i2c 400khz clk
csl_port         dat_100(inout);            // i2c 100khz data
csl_port         clk_100(output);            // i2c 100khz clk
i2c_ifc () { }
};

csl_interface clk_ifc { 
csl_port         sclk(output);               // clock pll serial clock
csl_port         data(output);               // clock pll serial data
csl_port         strobe_m(output);           // clock pll master strobe
csl_port         strobe_s(output);           // clock pll slave strobe
clk_ifc () { }
};

