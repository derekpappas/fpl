<?xml version = "1.0" ?>
<?xml-stylesheet type="text/xsl" href="unit.xsl"?>
<design>
<unit name="i2c_cluster">
<interface name="if_sff_fab" >
<port name="bus80" offset="0" upper="79" lower="0" direction = "output" type = "wire" />
</interface>
<interface name="af_if_fab" >
<port name="bus80" offset="0" upper="79" lower="0" direction = "input" type = "wire" />
</interface>
<signalgroup name="if_il_lba">
<signal name="dummy2" offset="0" upper="0" lower="0" type = "wire"/>
</signalgroup>
<signalgroup name="il_i_lb">
<signal name="dummy3" offset="0" upper="0" lower="0" type = "wire"/>
</signalgroup>
<instantiation type = "msi_fabric_if">
<instance name="i2c_fab">
<f2a formal = "fab_add.bus80" actual = "if_sff_fab.bus80" />
<f2a formal = "fab_drop.bus80" actual = "af_if_fab.bus80" />
<f2a formal = "lba.dummy2" actual = "if_il_lba.dummy2" />
</instance>
</instantiation>
<instantiation type = "i2c">
<instance name="i2c">
<f2a formal = "lb.dummy3" actual = "il_i_lb.dummy3" />
</instance>
</instantiation>
<instantiation type = "i2c_lba">
<instance name="i2c_lba">
<f2a formal = "lb.dummy3" actual = "il_i_lb.dummy3" />
<f2a formal = "lba.dummy2" actual = "if_il_lba.dummy2" />
</instance>
</instantiation>
</unit>
</design>
